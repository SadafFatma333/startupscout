# StartupScout Environment Variables

# LangFuse Tracing Configuration
# Get these keys from: https://cloud.langfuse.com/project/cmgwpu3rr000ead070ulzh2ho/settings/keys
LANGFUSE_ENABLED=true
LANGFUSE_HOST=https://cloud.langfuse.com
LANGFUSE_PUBLIC_KEY=pk_lf_your_public_key_here
LANGFUSE_SECRET_KEY=sk_lf_your_secret_key_here
LANGFUSE_SAMPLING_RATE=1.0

# Other required environment variables
OPENAI_API_KEY=your_openai_key_here
REDDIT_CLIENT_ID=your_reddit_client_id
REDDIT_USER_AGENT=your_user_agent
